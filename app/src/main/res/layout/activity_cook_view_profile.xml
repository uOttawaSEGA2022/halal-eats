<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CookViewProfile">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/cookfirstname"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="tobeautofilled" />

        <TextView
            android:id="@+id/cooklastname"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:text="tobeautofilled" />

        <TextView
            android:id="@+id/cookaddress"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:text="tobeautofilled" />

        <TextView
            android:id="@+id/cookpassword"
            android:layout_width="match_parent"
            android:layout_height="43dp"
            android:text="tobeautofilled" />

        <TextView
            android:id="@+id/cookdescription"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:text="tobeautofilled" />

        <TextView
            android:id="@+id/cookrating"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:text="tobeautofilled" />

        <TextView
            android:id="@+id/cooknumberofmealssold"
            android:layout_width="match_parent"
            android:layout_height="49dp"
            android:text="tobeautofilled" />

        <Button
            android:id="@+id/returnhome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="return home" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>